{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/cifrado/index.html",
        "destination": "/cifrado/"
      },
      {
        "source": "**/*.html",
        "destination": "/$1"
      },
      {
        "source": "/rock-paper-scissors/index.html",
        "destination": "/rock-paper-scissors/"
      },
      {
        "source": "/tictactoe/index.html",
        "destination": "/tictactoe"
      }           
    ]
  }
}
